QPushButton {
  background-color: #006666;
  color: #fff;
  font-weight: 600;
  border-radius: 8px;
  border: 1px solid #0d6efd;
  padding: 5px 15px;
  min-width: 100px;
}
QPushButton:hover {
  background-color: #333333;
  border: 2px solid #42a5f5;
}
QPushButton:pressed {
  background-color: #999999;
  border-style: inset;
}
QPushButton:disabled {
  background-color: #e0e0e0;
  color: #9e9e9e;
}
